import ipaddress
import pandas as pd

# List of networks
networks = [
    '10.76.0.0/16',
    '10.76.0.0/19',
    '10.76.32.0/19',
    '10.78.0.0/16',
    '10.78.0.0/25',
    '10.78.1.0/25',
    '10.78.1.128/25',
    '10.78.11.0/26',
    '10.78.11.64/26',
    '10.78.12.0/26',
    '10.78.13.0/26',
    '10.78.13.64/26',
    '10.78.16.0/22',
    '10.78.22.0/23',
    '10.78.24.0/24',
    '10.78.26.0/23',
    '10.78.31.0/24',
    '10.78.32.0/23',
    '10.78.34.0/23',
    '10.78.36.0/24',
    '10.78.37.0/24',
    '10.78.38.0/24',
    '10.78.39.0/24',
    '10.78.40.0/23',
    '10.78.42.0/23',
    '10.78.44.0/24',
    '10.78.45.0/24',
    '10.78.46.0/24',
    '10.78.47.0/24',
    '10.78.48.0/24',
    '10.78.49.0/24',
    '10.78.50.0/24',
    '10.78.51.0/24',
    '10.78.52.0/23',
    '10.78.54.0/23',
    '10.78.56.0/23',
    '10.78.58.0/23'
]

# Function to get first and last available IP in the network
def get_first_last_available_ip(network):
    net = ipaddress.ip_network(network)
    first_ip = net[10]  # The first 10 IPs are not usable
    last_ip = net[-2]  # The last usable IP
    return str(first_ip), str(last_ip)

# Create a list of dictionaries to store the results
results = []

for network in networks:
    first_ip, last_ip = get_first_last_available_ip(network)
    results.append({'Network': network, 'First Available IP': first_ip, 'Last Available IP': last_ip})

# Create a DataFrame from the results
df = pd.DataFrame(results)

# Display the DataFrame to the user
import ace_tools as tools; tools.display_dataframe_to_user(name="Available IPs in Networks", dataframe=df)
